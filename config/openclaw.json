{
  "env": {
    "OPENROUTER_API_KEY": "${OPENROUTER_API_KEY}"
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "openrouter/google/gemini-2.5-flash"
      }
    }
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "auth": {
      "mode": "token",
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    }
  },
  "skills": {
    "entries": {
      "weather": { "enabled": true },
      "jq": { "enabled": true },
      "domaindetails": { "enabled": true },
      "tldr": { "enabled": true },
      "shorten": { "enabled": true },
      "xkcd": { "enabled": true },
      "github": { "enabled": true },
      "notebook": { "enabled": true },
      "todo-tracker": { "enabled": true }
    }
  }
}